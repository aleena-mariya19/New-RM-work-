class Student:
    def __init__(self, name, grade):
        self.name = name
        self.grade = grade

    def display(self):
        print(f"Name: {self.name}, Grade: {self.grade}")

# Create an object
student1 = Student("Alice", "A")
student2 = Student("malavika","B")
student1.display()
student2.display()

Name: Alice, Grade: A
Name: malavika, Grade: B

